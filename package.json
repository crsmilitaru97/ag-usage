{
  "name": "ag-usage",
  "version": "1.3.1",
  "displayName": "AG Usage",
  "description": "Track usage of Antigravity AI models",
  "author": "Cristian Militaru",
  "publisher": "crsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/crsmilitaru97/ag-usage"
  },
  "license": "ISC",
  "keywords": [
    "antigravity",
    "ai",
    "usage",
    "quota",
    "monitor"
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "AI",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ag-usage.refresh",
        "title": "Refresh AG Usage",
        "icon": "$(refresh)"
      },
      {
        "command": "ag-usage.openSettings",
        "title": "Open AG Usage Settings",
        "icon": "$(gear)"
      }
    ],
    "configuration": {
      "title": "AG Usage",
      "properties": {
        "ag-usage.refreshInterval": {
          "type": "number",
          "default": 60,
          "minimum": 10,
          "description": "Refresh interval in seconds (minimum 10s). Set to 0 to disable auto-refresh."
        },
        "ag-usage.statusBarDisplay": {
          "type": "string",
          "default": "average",
          "enum": [
            "average",
            "all",
            "geminiPro",
            "geminiFlash",
            "claudeGpt"
          ],
          "enumDescriptions": [
            "Show the average usage across all groups",
            "Show all three groups side by side",
            "Show only Gemini 3 Pro usage",
            "Show only Gemini 3 Flash usage",
            "Show only Claude/GPT usage"
          ],
          "description": "What to display in the status bar."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "npx vsce package",
    "publish:ovsx": "npx ovsx publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/node": "25.0.3",
    "typescript": "^5.9.3"
  }
}